<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@id/profile"
    app:startDestination="@id/profileFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.nihalmistry.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile"
        >
        <action
            android:id="@+id/action_profileFragment_to_pofileDetailFragment"
            app:destination="@id/pofileDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/pofileDetailFragment"
        android:name="com.nihalmistry.profile.PofileDetailFragment"
        android:label="fragment_pofile_detail"
        tools:layout="@layout/fragment_pofile_detail" />
</navigation>